<Window x:Class="WpfApp1.WindowDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="WindowDelete" Height="272" Width="270" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="textBox_Id" HorizontalAlignment="Center" Margin="0,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="26"/>
        <Label Content="ID" HorizontalAlignment="Left" Margin="49,39,0,0" VerticalAlignment="Top"/>
        <Button Content="Удалить" HorizontalAlignment="Center" Margin="0,98,0,0" VerticalAlignment="Top" Height="30" Width="120" Click="Button_Click"/>

    </Grid>
</Window>
